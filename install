sudo apt-get update && sudo apt-get install automake autoconf make gcc fpc libsdl2-image-dev libavformat-dev libswscale-dev libsqlite3-dev libfreetype6-dev libportmidi-dev liblua5.3-dev libopencv-highgui-dev
./autogen.sh
./configure
make
make install
cp dists/linux/WorldParty.desktop /usr/share/applications/WorldParty.desktop
